{
    "Microsoft.Synapse/workspaces/credentials": {
      "properties": {
        "typeProperties": {
          "resourceId": "=:userManagedIdentityId:string"
        }
      }
    },
    "Microsoft.Synapse/workspaces/notebooks": {
      "properties": {
        "bigDataPool": {
          "referenceName": "=:bigDataPoolReference:string"
        }
      }
    },
    "Microsoft.Synapse/workspaces/pipelines": {
      "properties": {
        "parameters": {
          "database_name": {
            "defaultValue": "=:databaseName:string"
          }
        }
      }
    },
    "Microsoft.Synapse/workspaces/linkedServices": {
      "AzureBlobFS": {
        "properties": {
          "typeProperties": {
            "url": "=:dfsEndpointUri:string"
          }
        }
      },
      "AzureSqlDw": {
        "properties": {
          "typeProperties": {
            "connectionString": "=:azureSqlDwConnectionString:string"
          }
        },
        "parameters": {
          "DBName": {
            "defaultValue": "=:azureSqlDwDbNameDefault:string"
          }
        }
      },
      "AzureKeyVault": {
        "properties": {
          "typeProperties": {
            "baseUrl": "=:azureKeyVaultBaseUrl:string"
          }
        }
      },
      "*": {
        "properties": {
          "typeProperties": {
            "*": "="
          }
        }
      }
    },
    "Microsoft.Synapse/workspaces/sqlscripts": {
      "properties": {
        "content": {
          "currentConnection": {
            "*": "="
          }
        }
      }
    },
    "Microsoft.Synapse/workspaces/integrationRuntimes": {
      "properties": {
        "typeProperties": {
          "computeProperties": {
            "location": "=:region:string"
          }
        }
      }
    }
  }